        "datetime": "2025-12-12T22:54:22+00:00"
    },
    {
        "n": 15,
        "text": " ",
        "file": "DE\/0015.mp3",
        "lang": "DE",
        "datetime": "2025-12-12T22:54:22+00:00"
    },
    {
        "n": 16,
        "text": "Guten Nachmittag!",
        "file": "DE\/0016.mp3",
        "lang": "DE",
        "datetime": "2025-12-12T22:54:24+00:00"
    },
    {
        "n": 17,
        "text": "Ich lese und höre Musik,",
        "file": "DE\/0017.mp3",
        "lang": "DE",
        "datetime": "2025-12-12T22:54:26+00:00"
    },
    {
        "n": 18,
        "text": "oder ich gehe spazieren.",
        "file": "DE\/0018.mp3",
        "lang": "DE",
        "datetime": "2025-12-12T22:54:27+00:00"
    },
    {
        "n": 19,
        "text": " ",
