    },
    {
        "n": 3,
        "text": "Buongiorno!",
        "file": "IT\/0003.mp3",
        "lang": "IT",
        "datetime": "2025-12-12T22:55:19+00:00"
    },
    {
        "n": 4,
        "text": "È giorno.",
        "file": "IT\/0004.mp3",
        "lang": "IT",
        "datetime": "2025-12-12T22:55:21+00:00"
    },
    {
        "n": 5,
        "text": "Il sole brilla.",
        "file": "IT\/0005.mp3",
        "lang": "IT",
        "datetime": "2025-12-12T22:55:22+00:00"
    },
    {
        "n": 6,
        "text": " ",
        "file": "IT\/0006.mp3",
        "lang": "IT",
        "datetime": "2025-12-12T22:55:22+00:00"
    },
    {
        "n": 7,
        "text": "Mi alzo.",
        "file": "IT\/0007.mp3",
        "lang": "IT",
        "datetime": "2025-12-12T22:55:24+00:00"
    },
    {
        "n": 8,
        "text": "Mi lavo il viso.",
        "file": "IT\/0008.mp3",
        "lang": "IT",
        "datetime": "2025-12-12T22:55:25+00:00"
    },
    {
        "n": 9,
        "text": "Faccio un caffè",
        "file": "IT\/0009.mp3",
        "lang": "IT",
        "datetime": "2025-12-12T22:55:26+00:00"
    },
    {
        "n": 10,
        "text": "e faccio colazione un po'.",
        "file": "IT\/0010.mp3",
        "lang": "IT",
        "datetime": "2025-12-12T22:55:27+00:00"
    },
    {
        "n": 11,
        "text": " ",
        "file": "IT\/0011.mp3",
        "lang": "IT",
        "datetime": "2025-12-12T22:55:28+00:00"
    },
    {
        "n": 12,
        "text": "A pranzo",
        "file": "IT\/0012.mp3",
        "lang": "IT",
        "datetime": "2025-12-12T22:55:29+00:00"
    },
    {
        "n": 13,
        "text": "mangio pane e frutta.",
        "file": "IT\/0013.mp3",
        "lang": "IT",
        "datetime": "2025-12-12T22:55:30+00:00"
    },
    {
        "n": 14,
        "text": "Dopo riposo.",
        "file": "IT\/0014.mp3",
        "lang": "IT",
        "datetime": "2025-12-12T22:55:31+00:00"
    },
    {
        "n": 15,
        "text": " ",
        "file": "IT\/0015.mp3",
        "lang": "IT",
        "datetime": "2025-12-12T22:55:32+00:00"
    },
    {
        "n": 16,
        "text": "Buon pomeriggio!",
        "file": "IT\/0016.mp3",
        "lang": "IT",
        "datetime": "2025-12-12T22:55:33+00:00"
    },
    {
        "n": 17,
        "text": "Leggo e ascolto musica,",
        "file": "IT\/0017.mp3",
        "lang": "IT",
        "datetime": "2025-12-12T22:55:34+00:00"
    },
    {
        "n": 18,
        "text": "oppure esco a camminare.",
        "file": "IT\/0018.mp3",
        "lang": "IT",
        "datetime": "2025-12-12T22:55:36+00:00"
    },
    {
        "n": 19,
        "text": " ",
        "file": "IT\/0019.mp3",
        "lang": "IT",
        "datetime": "2025-12-12T22:55:36+00:00"
    },
    {
        "n": 20,
        "text": "Buonanotte!",
        "file": "IT\/0020.mp3",
        "lang": "IT",
        "datetime": "2025-12-12T22:55:37+00:00"
    },
    {
        "n": 21,
        "text": "La luna è nel cielo.",
        "file": "IT\/0021.mp3",
        "lang": "IT",
        "datetime": "2025-12-12T22:55:39+00:00"
    },
    {
        "n": 22,
        "text": "Chiudo gli occhi.",
        "file": "IT\/0022.mp3",
        "lang": "IT",
        "datetime": "2025-12-12T22:55:40+00:00"
    },
    {
        "n": 23,
        "text": "Sogno cose belle.",
        "file": "IT\/0023.mp3",
        "lang": "IT",
        "datetime": "2025-12-12T22:55:41+00:00"
    }
]